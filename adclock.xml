<adclock>
	<!-- path is relative to /usr/share/zoneinfo -->
	<timezone>Australia/Sydney</timezone>
	
	<hours>
		<!-- calibration factors are defined as (DAC value for max deflection) / (max digit on meter) -->
		<tenscal></tenscal>
		<unitscal></unitscal>
	</hours>
	
	<minutes>
		<tenscal></tenscal>
		<unitscal></unitscal>
	</minutes>
	
	<seconds>
		<tenscal></tenscal>
		<unitscal></unitscal>
	</seconds>
	
</adclock>
