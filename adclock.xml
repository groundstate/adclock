<adclock>
	<!-- path is relative to /usr/share/zoneinfo -->
	<timezone>Australia/Sydney</timezone>
	
	<hours>
		<!-- calibration factors are defined as (max_reading/full_scale_reading)*255 -->
		<tenscal></tenscal>
		<unitscal></unitscal>
	</hrs>
	
	<minutes>
		<tenscal></tenscal>
		<unitscal></unitscal>
	</mins>
	
	<seconds>
		<tenscal></tenscal>
		<unitscal></unitscal>
	</secs>
	
</adclock>